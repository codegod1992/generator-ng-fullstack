    "gulp",
    "mongodb",
    "fullstack",
    "go",
    "http2",
    "node"
  ],
  "dependencies": {
    "chalk": "^1.0.0",
    "lodash": "^4.13.1",
    "yeoman-generator": "^0.19.2",
    "yosay": "^1.2.0"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "coveralls": "^2.11.8",
    "eslint": "^3.0.1",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^5.1.1",
    "mocha-lcov-reporter": "^1.2.0",
    "sinon": "^1.17.1",
