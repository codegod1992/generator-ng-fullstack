{
    "quiet": true,
    "port": 3000,
    "http2": <%= secure %>,
    "pathIndex": "client/dev",
    "only": [
        "client/dev/**/*"
    ],
    "proxy": true,
    "proxyWhen": "/api/*",
    "static": [
        "node_modules",
        "jspm_packages"
    ],
    <% if (!secure) { %>
    "proxyTarget": "http://localhost:3333"
    <% } else { %>
    "proxyTarget": "https://localhost:3333"
    <% } %>
}
